{
  "name": "@gv-tech/ui-core",
  "root": "packages/ui-core",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "packages/ui-core"
      }
    },
    "nx-release-publish": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --provenance --access public",
        "cwd": "packages/ui-core"
      }
    }
  }
}
