{
  "name": "@gv-tech/design-system",
  "version": "2.10.0",
  "description": "Garcia Ventures react design system",
  "repository": {
    "type": "git",
    "url": "https://github.com/Garcia-Ventures/gvtech-design.git"
  },
  "license": "MIT",
  "author": "Eric N. Garcia <eng618@garciaericn.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./style.css": {
      "style": "./dist/design-system.css",
      "import": "./dist/design-system.css",
      "default": "./dist/design-system.css"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.es.js",
      "default": "./dist/*.es.js"
    }
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/design-system.css",
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && bun build:registry && VITE_LIB=true vite build",
    "build:registry": "tsx scripts/build-registry.ts",
    "build:site": "bun --filter playground-web build",
    "dev": "bun --filter playground-web dev",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:report": "eslint . --cache -o ./eslintReport.html -f html",
    "native": "npx nx run native-playground:start",
    "native:android": "npx nx run native-playground:start --android",
    "native:clear": "npx nx run native-playground:start --android --clear",
    "prepare": "husky",
    "start": "vite",
    "sync-tokens": "tsx scripts/sync-tokens.ts",
    "test": "vitest --passWithNoTests",
    "test:ci": "CI=true vitest --run --reporter=dot --passWithNoTests",
    "test:watch": "vitest --watch",
    "validate": "tsx ./scripts/validate.ts",
    "validate:fix": "tsx ./scripts/validate.ts --fix",
    "validate:no-cache": "tsx ./scripts/validate.ts --no-cache"
  },
  "lint-staged": {
    "*.(js|ts|mjs|cjs)?(x)": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.(md)?(x)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead"
    ]
  },
  "prettier": "@eng618/prettier-config",
  "resolutions": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^10.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "dependencies": {
    "@expo/metro-config": "~54.0.9",
    "@expo/metro-runtime": "~6.1.2",
    "@rn-primitives/types": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "expo": "~54.0.0",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "lucide-react-native": "^0.574.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-native": "0.81.5",
    "react-native-svg": "^15.15.3",
    "react-native-svg-transformer": "~1.5.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@eng618/prettier-config": "^2.9.0",
    "@expo/cli": "~54.0.16",
    "@gv-tech/eslint-config": "^0.1.8",
    "@nx/expo": "22.5.1",
    "@nx/jest": "22.5.2",
    "@nx/js": "^22.5.1",
    "@rn-primitives/slot": "^1.2.0",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.5",
    "@swc/helpers": "~0.5.18",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-native": "^0.73.0",
    "@types/react-native-web": "^0.19.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "axe-core": "^4.11.1",
    "esbuild-plugin-jsx": "^1.0.1",
    "eslint": "^10.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "metro-config": "~0.83.0",
    "metro-resolver": "~0.83.0",
    "next-themes": "^0.4.6",
    "nx": "^22.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-jsdoc": "^1.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-icons": "^5.5.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-web": "^0.21.2",
    "react-router-dom": "^7.13.0",
    "recharts": "2.15.4",
    "rollup-plugin-esbuild": "^6.2.1",
    "sonner": "^2.0.7",
    "tailwindcss": "4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "next-themes": "^0.4.0",
    "prop-types": "^15.8.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-is": "^16 || ^17 || ^18 || ^19",
    "react-native": "^0.77.0",
    "react-native-svg": "^15.11.1"
  },
  "packageManager": "bun@1.2.2",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
